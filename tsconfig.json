{
  "compilerOptions": {
    "module": "commonjs",
    "target": "es5",
    "lib": [
      "es2015",
      "es2017",
      "dom"
    ],
    "noImplicitAny": false,
    "sourceMap": false,
    "declaration": false,
    "strictNullChecks": false,
    "suppressImplicitAnyIndexErrors": false,
    "outDir": "./lib",
    "moduleResolution": "node"
  },
  "exclude": [
    "node_modules",
    "static"
  ]
}
